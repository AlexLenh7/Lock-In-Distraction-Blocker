{
  "manifest_version": 3,
  "name": "Distraction blocker",
  "version": "1.0",
  "description": "blocks distractions",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs", "activeTab", "alarms"]
}
